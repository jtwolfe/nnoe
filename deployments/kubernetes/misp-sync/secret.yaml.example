# Example Secret for MISP credentials
# Create this file as misp-credentials.yaml and kubectl apply it
# DO NOT commit actual credentials to git

apiVersion: v1
kind: Secret
metadata:
  name: misp-credentials
  namespace: nnoe
type: Opaque
stringData:
  url: "https://misp.example.com"
  api-key: "your-misp-api-key-here"
  # Optional: Secondary MISP instance
  # url-2: "https://misp2.example.com"
  # api-key-2: "your-second-api-key-here"

